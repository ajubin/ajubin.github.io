{
	"name": "ajubin-blog",
	"version": "1.0.0",
	"description": "My personnal website",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy ",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"lint": "eslint . --ext .js,.json,.md,.css --fix"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ajubin/ajuin.github.io.git"
	},
	"author": {
		"name": "ajubin"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"homepage": "https://ajubin.github.io/",
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-img": "^6.0.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^2.0.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"@eslint/css": "^0.6.0",
		"@eslint/js": "^9.24.0",
		"@eslint/json": "^0.11.0",
		"@eslint/markdown": "^6.3.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.24.0",
		"globals": "^16.0.0",
		"luxon": "^3.5.0",
		"prettier": "^3.5.3",
		"prismjs": "^1.29.0",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.1"
	},
	"browserslist": [
		"last 2 versions",
		"not dead",
		"supports @view-transition",
		"supports clip",
		"supports hyphens"
	]
}
